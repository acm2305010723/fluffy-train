#include<stdio.h>
int main()
{
   int a;
   int i;
   scanf("%d",&a);
   if(a>=0 && a<8)
   {
   	i=8-a;
   }
   if(a>=8 && a<88)
   {
   	i=88-a;
   }
   if(a>=88 && a<888)
   {
   	i=888-a;
   }
   if(a>=888 && a<8888)
   {
   	i=8888-a;
   }
   if(a>=8888 && a<=9999)
   {
   	i=88888-a;
   }
   printf("%d",i);
   return 0;	
