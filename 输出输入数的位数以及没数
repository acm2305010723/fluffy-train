#include<stdio.h>
int main()
{
	long long s;
	int count;
	int a,b,c,d,e;
	scanf("%lld",&s);
	a=s%10;
	b=s/10%10;
	c=s/100%10;
	d=s/1000%10;
	e=s/10000%10;
	while(s!=0)
	{ 
	    s/=10;
		count++;
	}
	printf("%d\n",count);
	printf("%d %d %d %d %d\n",e,d,c,b,a);
	printf("%d%d%d%d%d\n",a,b,c,d,e);
	return 0;
}
